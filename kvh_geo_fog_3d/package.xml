<package format="2">
  <name>kvh_geo_fog_3d</name>
  <version>1.3.3</version>
  <description>Provides a driver node for KVH GEO FOG 3D INS sensors, messages, and rviz plugins.</description>
  <author email="zlacelle@mitre.org">Zach LaCelle</author>
  <maintainer email="zlacelle@mitre.org">Zach LaCelle</maintainer>
  <license>Apache 2.0</license>

  <buildtool_depend>catkin</buildtool_depend>

  <exec_depend>kvh_geo_fog_3d_msgs</exec_depend>
  <exec_depend>kvh_geo_fog_3d_driver</exec_depend>
  <exec_depend>kvh_geo_fog_3d_rviz</exec_depend>
  <!-- The export tag contains other, unspecified, tags -->
  <export>
    <rosdoc external="http://www.kvh.com/admin/products/gyros-imus-inss/ins/geo-fog-3d-dual/commercial-geo-fog-3d-dual/"/>
    <metapackage />
  </export>
</package>
